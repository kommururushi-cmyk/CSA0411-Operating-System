#include <stdio.h>

int main() {
    int n;
    printf("Enter number of blocks: ");
    scanf("%d",&n);
    int index[n];
    printf("Enter block numbers for index block:\n");
    for(int i=0;i<n;i++) scanf("%d",&index[i]);
    printf("File blocks via index:\n");
    for(int i=0;i<n;i++) printf("Block %d\n", index[i]);
    return 0;
}
